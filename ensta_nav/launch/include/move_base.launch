<?xml version="1.0"?>
<launch>

    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
        <param name="base_global_planner" value="global_planner/GlobalPlanner" />

        <rosparam file="$(find ensta_nav)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find ensta_nav)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />

        <rosparam file="$(find ensta_nav)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find ensta_nav)/param/global_costmap_params.yaml" command="load" />

        <rosparam file="$(find ensta_nav)/param/move_base_params.yaml" command="load" />

        <rosparam file="$(find ensta_nav)/param/dwa_local_planner_params.yaml" command="load" />

        <rosparam file="$(find ensta_nav)/param/navfn_global_planner_params.yaml" command="load" />
        <rosparam file="$(find ensta_nav)/param/global_planner_params.yaml" command="load" />
    </node>

</launch>

